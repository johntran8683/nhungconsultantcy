{
  "name": "nhung-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXTAUTH_URL=http://localhost:3000 NEXT_PUBLIC_APP_URL=http://localhost:3000 NEXTAUTH_SECRET=dev-secret-please-change DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nhung?schema=public EMAIL_PROVIDER=brevo BREVO_API_KEY=test_dummy EMAIL_FROM='Nhung Consultancy <contact@nhungconsultancy.com>' next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "docker:dev": "docker compose up --build",
    "docker:dev:detached": "docker compose up --build -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f app",
    "docker:db:migrate": "docker compose exec app npx prisma migrate deploy",
    "docker:db:seed": "docker compose exec app npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "docker:db:reset": "docker compose exec app ./scripts/reset-db.sh",
    "docker:db:reset:force": "docker compose exec app ./scripts/reset-db.sh --force",
    "docker:db:studio": "docker compose --profile tools up prisma-studio",
    "docker:restart": "docker compose restart app",
    "docker:rebuild": "docker compose down && docker compose up --build",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.3",
    "@types/nodemailer": "^7.0.2",
    "bcrypt": "^6.0.0",
    "lucide-react": "^0.263.1",
    "next": "14.2.5",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "^18",
    "react-dom": "^18",
    "react-intersection-observer": "^9.5.2",
    "resend": "^6.1.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prisma": "^6.16.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
